
@{
    ViewBag.Title = "AeronavesDisponibles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section>
    <h2>Aeronaves disponibles</h2>
    <p id="codigoUsuario" style="display:none;">@ViewBag.codigo</p>
</section>
<div class="row">
    <div class="col-md-12 table-responsive">
        <div class="box box-primary">
            <div class="box-body display table-responsive">
                <table id="tableAeronaves" class="table table-striped table-responsive">
                    <thead>
                        <tr>
                            <th>Matricula</th>
                            <th>Horas vuelos</th>
                            <th>Fecha inspección</th>
                            <th>Fecha aeronavegabilidad</th>
                            <th>Año fabricación</th>
                            <th>Tipo aeronave</th>
                            <th>Estado aeronave</th>
                        </tr>
                    </thead>
                    <tbody id="bodyTableAeronaves">

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

