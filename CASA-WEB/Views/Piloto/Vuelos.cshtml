
@{
    ViewBag.Title = "Vuelos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section>
    <h2>Vuelos</h2>
    <p id="codigoUsuario" style="display:none;">@ViewBag.codigo</p>
</section>
<div class="row">
    <div class="col-md-12 table-responsive">
        <div class="box box-primary">
            <div class="box-body display table-responsive">
                <table id="tableVuelos" class="table table-striped table-responsive">
                    <thead>
                        <tr>
                            <th>Rut Piloto</th>
                            <th>Rut Copiloto</th>
                            <th>Matricula aeronave</th>
                            <th>fecha vuelo</th>
                            <th>Condición vuelo</th>
                            <th>Horas totales</th>
                            <th>Horas Pilito</th>
                            <th>Horas Copiloto</th>
                            <th>Origen</th>
                            <th>Destino</th>
                            <th>Misión</th>
                        </tr>
                    </thead>
                    <tbody id="bodyTableVuelos">

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


